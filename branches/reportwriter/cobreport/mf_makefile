MYTIP=..
include $(TIPSRC)/Make.def
include $(TIPSRC)/make.$(TARGET_OS)

CFLAGS= $(CFLGS)
FLAGS = -P -Ox -C "NOWARNING VSC2 IBMCOMP NOTRUNC" 
LDBAT=	-L$(TIPROOT)/$(LIBDIR) -lbat -ltip $(MFLIB) -lm -lnsl 

.bat:
	cob $(FLAGS)  -k $(@F).bat -o $(@F) $(LDBAT)
	$(RM) $(@F).o $(@F).idy $(@F).int $(@F).lst
