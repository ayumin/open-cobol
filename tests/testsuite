484;run_functions.at:2004;FUNCTION SUBSTITUTE-CASE with reference mod;functions;
485;run_functions.at:2031;FUNCTION SUM;functions;
486;run_functions.at:2054;FUNCTION TAN;functions;
487;run_functions.at:2077;FUNCTION TEST-DATE-YYYYMMDD;functions;
488;run_functions.at:2098;FUNCTION TEST-DAY-YYYYDDD;functions;
489;run_functions.at:2119;FUNCTION TEST-NUMVAL;functions;
