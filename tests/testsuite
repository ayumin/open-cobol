( $at_check_trace; $COMPILE -std=mf prog.cob
) >>"$at_stdout" 2>>"$at_stderr"
at_status=$? at_failed=false
$at_check_filter
echo >>"$at_stderr"; $as_echo "prog.cob: 32: Warning: LABEL RECORDS is obsolete in Micro Focus COBOL
prog.cob: 34: Warning: RECORD clause ignored for LINE SEQUENTIAL
prog.cob: 35: Warning: DATA RECORDS is obsolete in Micro Focus COBOL
prog.cob: 52: Warning: LABEL RECORDS is obsolete in Micro Focus COBOL
" | \
  $at_diff - "$at_stderr" || at_failed=:
at_fn_diff_devnull "$at_stdout" || at_failed=:
