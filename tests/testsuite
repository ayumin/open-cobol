       SELECT OPTIONAL fileX
           ASSIGN DISK
           ORGANIZATION INDEXED
           ACCESS MODE DYNAMIC
           RECORD KEY fileX-key
           STATUS f-status.
       DATA DIVISION.
       FILE SECTION.
