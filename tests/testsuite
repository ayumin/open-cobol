#AT_STOP_217
#AT_START_218
at_fn_group_banner 218 'run_file.at:1643' \
  "File ORG:REL ACC:SEQ IO with variable rsz" "      " 2
at_xfail=no
(
  $as_echo "218. $at_setup_line: testing $at_desc ..."
  $at_traceon



cat >prog.cob <<'_ATEOF'

       identification division.
       program-id. prog.
       environment division.
       input-output section.
       file-control.
       select file1 assign disk
          organization relative.
       data division.
       file section.
       fd file1 record varying from 10 to 20 depending w-rsz.
           1  file1-rec.
              2    file1-x occurs 20 pic x.
       working-storage section.
           77  w-rsz pic 99.
           77  w-sub pic 99.
           1   1-template pic x(20) value "+12345678++12345678+".
           1   filler redefines 1-template.
               2   1-x occurs 20 pic x.
       procedure division.
           open output file1.
           perform varying w-rsz from 20 by -1 until w-rsz < 10
              write file1-rec from 1-template
           end-perform.
           close file1.
           open input file1.
      *    w-rsz should not influence READ
           move 15 to w-rsz.
           perform varying w-sub from 20 by -1 until w-sub < 10
              read file1 at end
                 display "FAILED EOF"
                 stop run
              end-read
              display w-rsz ": >" file1-rec "<"
              if (w-rsz not = w-sub)
                 display "FAILED BAD RSZ"
                 stop run
              end-if
              if (file1-x(w-rsz) not = 1-x(w-rsz))
                 display "FAILED BAD DATA"
                 stop run
              end-if
           end-perform.
           close file1.
           stop run.
_ATEOF


