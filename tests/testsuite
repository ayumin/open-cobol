cat >prog.cob <<'_ATEOF'

       IDENTIFICATION   DIVISION.
       PROGRAM-ID.      prog.
       ENVIRONMENT      DIVISION.
       INPUT-OUTPUT     SECTION.
       FILE-CONTROL.
       SELECT TEST-FILE1
              ASSIGN        "TESTFILE"
              ACCESS        DYNAMIC
              ORGANIZATION  INDEXED
              RECORD KEY
                 TESTKEY1 = TESTKEY-P1
              ALTERNATE RECORD KEY
                 TESTKEY2 = TESTKEY-P2
                            TESTKEY-P3
                 WITH DUPLICATES
              .
       DATA             DIVISION.
       FILE             SECTION.
       FD  TEST-FILE.
       01  TEST-REC.
           03  TESTKEY-P1 PIC X(4).
           03  TESTKEY-P2 PIC 9(4).
           03  TESTDATA   PIC X(4).
           03  TESTKEY-P3 PIC X(4).
       PROCEDURE        DIVISION.
           OPEN  INPUT TEST-FILE.
           READ  TEST-FILE KEY IS TESTKEY1
           END-READ.
           START TEST-FILE KEY IS GREATER THAN TESTKEY2
           END-START.
           READ  TEST-FILE NEXT
           END-READ.
           CLOSE TEST-FILE.
           STOP RUN.
       END PROGRAM prog.
_ATEOF


