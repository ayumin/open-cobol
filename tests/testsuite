#AT_STOP_235
#AT_START_236
at_fn_group_banner 236 'run_file.at:2965' \
  "File ORG:IDX ACC:DYN Split-keys (2)" "            " 2
at_xfail=no
(
  $as_echo "236. $at_setup_line: testing $at_desc ..."
  $at_traceon



cat >prog.cob <<'_ATEOF'

       IDENTIFICATION   DIVISION.
       PROGRAM-ID.      prog.
       ENVIRONMENT      DIVISION.
       INPUT-OUTPUT     SECTION.
       FILE-CONTROL.
       SELECT TEST-FILE
              ASSIGN        "TESTFILE"
              ACCESS        DYNAMIC
              ORGANIZATION  INDEXED
              RECORD KEY           TESTKEY1 SOURCE IS TESTKEY-P1
              ALTERNATE RECORD KEY TESTKEY2 SOURCE IS TESTKEY-P2,
                                                      TESTKEY-P3
                                            WITH DUPLICATES
              .
       DATA             DIVISION.
       FILE             SECTION.
       FD  TEST-FILE.
       01  TEST-REC.
           03  TESTKEY-P1 PIC X(4).
           03  TESTKEY-P2 PIC 9(4).
           03  TESTDATA   PIC X(4).
           03  TESTKEY-P3 PIC X(4).
       PROCEDURE        DIVISION.
           OPEN  OUTPUT TEST-FILE.
           WRITE TEST-REC FROM "BBBB0001dat1aaaa"
           END-WRITE.
           WRITE TEST-REC FROM "AAAA0001dat2aaaa"
           END-WRITE.
           WRITE TEST-REC FROM "CCCC0002dat3aaaa"
           END-WRITE.
           WRITE TEST-REC FROM "DDDD0002dat4bbbb"
           END-WRITE.
           CLOSE TEST-FILE.
           OPEN  INPUT TEST-FILE.
           MOVE  "CCCC" TO TESTKEY-P1.
           READ  TEST-FILE KEY IS TESTKEY1
              INVALID KEY
                 DISPLAY "READ with CCCC found no record"
                 END-DISPLAY
              NOT INVALID KEY
                 IF TESTDATA NOT = "dat3"
                    DISPLAY "READ with wrong result: "
                            TEST-REC
                    END-DISPLAY
                 END-IF
           END-READ.
           MOVE 0001     TO TESTKEY-P2.
           MOVE "aaaa"   TO TESTKEY-P3.
           START TEST-FILE KEY >= TESTKEY2
              INVALID KEY
                 DISPLAY "START >= 0001 found no record"
                 END-DISPLAY
              NOT INVALID KEY
                 READ  TEST-FILE NEXT
                    AT END
                       DISPLAY "READ NEXT (1) found no record"
                       END-DISPLAY
                       GO TO END-PAR
                    NOT AT END
                       IF TESTDATA NOT = "dat2"
                          DISPLAY "READ NEXT (1) "
                                  "with wrong result: "
                                  TESTDATA
                          END-DISPLAY
                          GO TO END-PAR
                       END-IF
                 END-READ
                 READ  TEST-FILE NEXT
                    AT END
                       DISPLAY "READ NEXT (2) found no record"
                       END-DISPLAY
                       GO TO END-PAR
                    NOT AT END
                       IF TESTDATA NOT = "dat1"
                          DISPLAY "READ NEXT (2) "
                                  "with wrong result: "
                                  TESTDATA
                          END-DISPLAY
                          GO TO END-PAR
                       END-IF
                 END-READ
           END-START.
           MOVE 0001      TO TESTKEY-P2.
           MOVE "aaaa"    TO TESTKEY-P3.
           START TEST-FILE KEY > TESTKEY2
              INVALID KEY
                 DISPLAY "START > 0001 found no record"
                 END-DISPLAY
              NOT INVALID KEY
                 READ  TEST-FILE NEXT
                    AT END
                       DISPLAY "READ NEXT (3) found no record"
                       END-DISPLAY
                       GO TO END-PAR
                    NOT AT END
                       IF TESTDATA NOT = "dat3"
                          DISPLAY "READ NEXT (3) "
                                  "with wrong result: "
                                  TESTDATA
                          END-DISPLAY
                          GO TO END-PAR
                       END-IF
                 END-READ
           END-START.
           MOVE 0002      TO TESTKEY-P2.
           MOVE ALL "Z"   TO TESTKEY-P3.
           START TEST-FILE KEY IS <= TESTKEY2
              INVALID KEY
                 DISPLAY "START <= 0002 found no record"
                 END-DISPLAY
              NOT INVALID KEY
                 READ  TEST-FILE PREVIOUS
                    AT END
                       DISPLAY "READ PREVIOUS (1) found no record"
                       END-DISPLAY
                       GO TO END-PAR
                    NOT AT END
                       IF TESTDATA NOT = "dat4"
                          DISPLAY "READ PREVIOUS (1) "
                                  "with wrong result: "
                                  TESTDATA
                          END-DISPLAY
                          GO TO END-PAR
                       END-IF
                 END-READ
                 READ  TEST-FILE PREVIOUS
                    AT END
                       DISPLAY "READ PREVIOUS (2) found no record"
                       END-DISPLAY
                       GO TO END-PAR
                    NOT AT END
                       IF TESTDATA NOT = "dat3"
                          DISPLAY "READ PREVIOUS (2) "
                                  "with wrong result: "
                                  TESTDATA
                          END-DISPLAY
                          GO TO END-PAR
                       END-IF
                 END-READ
           END-START.
       END-PAR.
           CLOSE TEST-FILE.
           STOP RUN.
       END PROGRAM prog.
_ATEOF


{ set +x
