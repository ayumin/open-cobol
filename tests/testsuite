#AT_STOP_224
#AT_START_225
at_fn_group_banner 225 'run_file.at:2107' \
  "File ORG:IDX ACC:SEQ IO with variable rsz" "      " 2
at_xfail=no
(
  $as_echo "225. $at_setup_line: testing $at_desc ..."
  $at_traceon



cat >prog.cob <<'_ATEOF'

       program-id. prog.
       environment division.
       input-output section.
       file-control.
       select file1 assign disk
          organization indexed
          record key file1-key
          access random.
       data division.
       file section.
       fd file1 record varying from 12 to 22 depending w-rsz.
           1  file1-rec.
              2    file1-key pic 99.
              2    file1-data.
               3   file1-x occurs 20 pic x.
       working-storage section.
           77  w-rsz pic 99.
           1   1-template pic x(20) value "+12345678++12345678+".
           1   filler redefines 1-template.
               2   1-x occurs 20 pic x.
       procedure division.
           open output file1.
           move 1 to file1-key.
           perform varying w-rsz from 22 by -1 until w-rsz < 12
              move 1-template to file1-data
              write file1-rec
              add 1 to file1-key
           end-perform.
           close file1.
           open input file1.
      *    w-rsz should not influence READ
           move 15 to w-rsz.
           perform varying file1-key from 1 by 1 until file1-key > 11
              read file1 at end
                 display "FAILED EOF"
                 stop run
              end-read
              subtract 2 from w-rsz
              display w-rsz ": >" file1-data "<"
              if (w-rsz not = (21 - file1-key))
                 display "FAILED BAD RSZ"
                 stop run
              end-if
              if (file1-x(w-rsz) not = 1-x(w-rsz))
                 display "FAILED BAD DATA"
                 stop run
              end-if
           end-perform.
           close file1.
           stop run.


_ATEOF


