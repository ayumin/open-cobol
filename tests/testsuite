       SELECT OPTIONAL file1
           ASSIGN DISK
           STATUS f-status.
       DATA DIVISION.
       FILE SECTION.
       FD file1 GLOBAL.
       1  file1-rec pic x(10).
       WORKING-STORAGE SECTION.
       77  f-status pic xx.
       PROCEDURE DIVISION.
           MOVE "xx" to f-status.
           CALL "prog2".
           IF (f-status <> "00")
              DISPLAY "FAILED: fs= " f-status
           ELSE
              CLOSE file1
           END-IF.
           STOP RUN.
      *
       IDENTIFICATION DIVISION.
       PROGRAM-ID. prog2.
       DATA DIVISION.
       WORKING-STORAGE SECTION.
       PROCEDURE DIVISION.
           OPEN OUTPUT file1.
           EXIT PROGRAM.
       END PROGRAM prog2.
       END PROGRAM prog.
_ATEOF


