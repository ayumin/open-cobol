AT_SETUP([Hexadecimal literal])
AT_KEYWORDS([fundamental])

AT_DATA([dump.c], [
#include <stdio.h>
#ifdef	__INTEL_COMPILER
#pragma warning ( disable : 1419 )
#endif
int dump (unsigned char *);
int dump (unsigned char *data)
{
  int i;
  for (i = 0; i < 4; i++)
    printf ("%02x", data[[i]]);
  puts ("");
  return 0;
}
])

AT_DATA([prog.cob], [
       IDENTIFICATION   DIVISION.
       PROGRAM-ID.      prog.
       PROCEDURE        DIVISION.
